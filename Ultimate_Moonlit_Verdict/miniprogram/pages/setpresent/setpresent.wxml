<block wx:if="{{PreIsShow[0]}}">
    <view class="addpre-box" >
        <view class="addpre-bg"></view>
        <view class="addpre-toast">
            <view class="addpre-bgin">
                <view class="preInput">
                    <text style="font-size: 32rpx; font-weight: 600; margin-left: 12rpx;" > 奖品名称 : </text>
                    <input bindinput="getOnepre" placeholder="请输入奖品名" class="inputt"></input>
                </view>
                <view class="addimgbox">
                    <text style="font-size: 32rpx; font-weight: 600; margin-left: 12rpx;" > 添加图片 : </text>
                    <image class="addimg" src="{{addOnepre}}" mode="aspectFit" catchtap="addOnepresent">  </image>
                </view>
                <view class="conbox">
                    <view class="coninbox" catchtap="canceladdOne">
                        <text style="font-size: 40rpx; font-weight: 600;" > 取消 </text>
                    </view>
                    <view class="coninbox" catchtap="confirmaddOne">
                        <text style="font-size: 40rpx; font-weight: 600;" > 确定 </text>
                    </view>
                </view>
            </view>
        </view>
    </view>
</block>
<!-- 添加奖品弹窗页面 状元 -->

