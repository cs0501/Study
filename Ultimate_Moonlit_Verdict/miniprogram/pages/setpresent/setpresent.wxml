<block class="状元">
    <view class="namebox">
        <text style="width: 12%; font-weight: 600;font-size: 42rpx;"> 状元 </text>
        <text style="width: 23%;font-size: 36rpx;">奖品数量: </text>
        <block wx:if="{{!(PreIsShow[0]||PreIsShow[1]||PreIsShow[2]||PreIsShow[3]||PreIsShow[4]||PreIsShow[5])}}">
            <input style=" width: 45%; background-color: whitesmoke; font-size: 36rpx;" bindinput="onePrizeNum" placeholder="请输入状元奖品总数"> </input>
        </block>
    </view>
    <scroll-view scroll-y class="scroll1" scroll-top>
        <view class="outerbox" wx:for="{{Onepre}}" wx:key="index">
            <view class="imgbox">
                <image src="{{item}}" mode="aspectFit" class="img"></image>
            </view>
            <view class="presentbox">
                <text>{{OnepreName[index]}}</text>
            </view>
        </view>
    </scroll-view>
    <view class="addOnebox">
        <view class="addOneboxplus" catchtap="addNumberOne">
            <image src="../../images/addpre.png" class="addpre" mode="aspectFit" ></image>
        </view>
    </view>
</block>

